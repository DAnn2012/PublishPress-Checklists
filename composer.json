{
  "name": "publishpress/publishpress-checklists",
  "type": "wordpress-plugin",
  "description": "",
  "license": "GPL-2",
  "authors": [
    {
      "name": "PublishPress",
      "email": "help@publishpress.com"
    }
  ],
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/publishpress/PublishPress-Plugin-Builder"
    },
    {
      "type": "git",
      "url": "https://github.com/publishpress/wp-browser"
    }
  ],
  "require": {
    "php": ">=5.6.20",
    "publishpress/pimple-pimple": "3.5.0.*",
    "publishpress/vendor-locator-checklists": "^1.0",
    "publishpress/wordpress-version-notices": "^2.1",
    "publishpress/wordpress-reviews":  "^1.1",
    "publishpress/instance-protection": "^2.0",
    "symfony/polyfill-ctype": "1.19.*"
  },
  "require-dev": {
    "publishpress/publishpress-plugin-builder": "^1.3",
    "publishpress/wp-browser": "2.6.5.2",
    "codeception/module-asserts": "^1.2",
    "codeception/module-phpbrowser": "^1.0",
    "codeception/module-db": "^1.0",
    "codeception/util-universalframework": "^1.0",
    "overtrue/phplint": "^2.4"
  },
  "scripts": {
    "build": "/project/dev-workspace/docker/scripts/ppbuild build",
    "build:dir": "/project/dev-workspace/docker/scripts/ppbuild build-dir",
    "build:clean": "/project/dev-workspace/docker/scripts/ppbuild clean",
    "get-version": "/project/dev-workspace/docker/scripts/ppbuild version"
  },
  "minimum-stability": "stable",
  "extra": {
    "plugin-slug": "publishpress-checklists",
    "plugin-name": "publishpress-checklists",
    "plugin-folder": "publishpress-checklists"
  }
}
